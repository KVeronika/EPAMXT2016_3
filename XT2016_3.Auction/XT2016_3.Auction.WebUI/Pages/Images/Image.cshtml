@using XT2016_3.Auction.WebUI.Models
@{
    if (IsPost)
    {
        Response.SetStatus(HttpStatusCode.NotFound);
        return;
    }

    int id = int.Parse(Request["Id"]);
    var photoData = LogicProvider.ImageLogic.GetData(id);

    Response.WriteBinary(photoData.Data, photoData.ContentType);
}
